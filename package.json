{
  "name": "help-yourself",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "lucide-react": "^0.536.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:backend": "cd backend && npm start",
    "start:dev": "concurrently \"npm run start:backend\" \"npm start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watchAll",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:coverage:watch": "react-scripts test --coverage",
    "test:ci": "react-scripts test --coverage --watchAll=false --reporters=default --reporters=jest-junit",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:update-snapshots": "react-scripts test --updateSnapshot --watchAll=false",
    "lint": "eslint src --ext .js,.jsx --fix",
    "lint:check": "eslint src --ext .js,.jsx",
    "build:netlify": "REACT_APP_API_URL=https://help-yourself-backend.onrender.com//api npm run build",
    "netlify:dev": "netlify dev",
    "netlify:deploy": "netlify deploy --prod --dir=build",
    "eject": "react-scripts eject",
    "seed-data": "node mockData.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6"
  }
}
